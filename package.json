{
  "name": "icon-helper",
  "version": "0.1.0",
  "private": true,
  "main": "electron/main.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"wait-on http://127.0.0.1:3000 && electron .\"",
    "build": "vite build",
    "start": "electron .",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.iconhelper.app",
    "productName": "IconPanda",
    "directories": {
      "buildResources": "build"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    }
  },
  "dependencies": {
    "@better-upload/client": "^3.0.2",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-select": "latest",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "electron-store": "^9.0.0",
    "lucide-react": "^0.553.0",
    "mac-scrollbar": "^0.13.8",
    "png-to-ico": "^3.0.1",
    "canvas-confetti": "^1.9.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^3.4.0",
    "win-lnk-parser": "^1.1.2",
    "windows-shortcuts-ps": "^1.0.1"
  },
  "devDependencies": {
    "@types/node": "^22.8.6",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.22",
    "concurrently": "^9.0.1",
    "electron": "^30.0.9",
    "electron-builder": "^24.13.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.12",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "wait-on": "^8.0.2",
    "@types/canvas-confetti": "^1.0.3"
  }
}
